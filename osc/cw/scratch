// Snippets that may be used later

// Pop head and update the pointer's value
void listRemove(struct process **head) {
  struct process *newHead = (*head)->oNext;
  free(*head);
  *head = newHead;
}

// Print out linked list of processes
void listPrint(struct process *head) {
  do {
    printf("ID=%d BurstTime=%d\n", head->iProcessId, head->iBurstTime);
  } while ((head = head->oNext));
}
